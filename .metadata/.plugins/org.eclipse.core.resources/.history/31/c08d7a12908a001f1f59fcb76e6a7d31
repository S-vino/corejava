package basic;

public class Merge2Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int []a= {1,2,3,4,5};
		int[] b= {6,7,8,9,10};
		int res[]=new int[a.length+b.length];
		for (int i=0;i<res.length;i++) {
			if(i/2==0) {
				res[i]=a[i];
			}
			else {
				res[i]=b[i];
			}
		}
	}

}
