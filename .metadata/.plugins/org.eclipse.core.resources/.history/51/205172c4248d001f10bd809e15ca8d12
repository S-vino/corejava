package basic;
import java.util.*;
public class FibAndSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//static Scanner sc=new Scanner(System.in);
		//System.out.println("enter the range of fibo");
		// int a=sc.nextInt();
		
		int a=5;
		int n1=0;
		int n2=1;
		int n3=1;
		int sum=1;
		System.out.print(n1+""+n2);
		for(int i=1;i<=a-2;i++)
		{
			n3=n1+n2;
			sum= sum+n3;
			System.out.print( " "+n3+" ");
			
			n1=n2;
			n2=n3;
		}
		System.out.println(sum);
	}

}
